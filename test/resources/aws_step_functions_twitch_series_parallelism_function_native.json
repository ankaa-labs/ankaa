{
    "Comment": "A Hello World example demonstrating various state types of the Amazon States Language",
    "StartAt": "Do Stuff",
    "States": {
      "Do Stuff": {
        "Type": "Task",
        "Resource": "arn:aws:states:::lambda:invoke",
        "Parameters": {
          "FunctionName": "${DoStuffFunctionArn}",
          "Payload": {
            "bucket": "${TestBucketName}",
            "prefix.$": "$.prefix"
          }
        },
        "End": true
      }
    }
  }